<odoo>
  <data>
    <record model="ir.ui.view" id="studio_customization.odoo_studio_project__cb719314-979a-4572-9865-fccd9c62d051">
      <field name="name">Odoo Studio: project.project.form customization</field>
      <field name="model">project.project</field>
      <field name="inherit_id" ref="project.edit_project"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
      <xpath expr="/form[1]/sheet[1]/div[2]" position="after">
        <field name="interpod_project_site"/>
      </xpath>
      <xpath expr="//field[@name='interpod_project_site'][not(ancestor::field)]" position="attributes">
        <attribute name="string">Project Site</attribute>
        <attribute name="groups"/>
        <attribute name="widget">url</attribute>
      </xpath>
      </field>
    </record>
  </data>
</odoo>
