    <record model="ir.ui.view" id="studio_customization.odoo_studio_stock_lo_196d0b1e-e25d-4d5b-9050-02c0597bb1a1">
      <field name="name">Odoo Studio: stock.location.tree customization</field>
      <field name="model">stock.location</field>
      <field name="inherit_id" ref="stock.view_location_tree2"/>
      <field name="type">tree</field>
      <field name="arch" type="xml">
      <xpath expr="//field[@name='display_name'][not(ancestor::field)]" position="before">
        <field name="id"/>
      </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="studio_customization.odoo_studio_stock_wa_d7b1d762-7d75-4765-8df1-678de06eed8a">
      <field name="name">Odoo Studio: stock.warehouse customization</field>
      <field name="model">stock.warehouse</field>
      <field name="inherit_id" ref="stock.view_warehouse"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
      <xpath expr="//field[@name='resupply_wh_ids'][not(ancestor::field)]" position="after">
        <field name="lot_stock_id"/>
      </xpath>
      <xpath expr="//field[@name='lot_stock_id'][not(ancestor::field)]" position="replace"/>
      <xpath expr="//field[@name='wh_output_stock_loc_id'][not(ancestor::field)]" position="after">
        <field name="lot_stock_id"/>
      </xpath>
      </field>
    </record>

